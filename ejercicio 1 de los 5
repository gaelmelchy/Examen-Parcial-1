//programa para imprimir 10 num


#include <stdio.h>
#include <math.h>

int main() {
    int numero;

    printf("Ingresa un numero: ");
    scanf("%d", &numero);

    printf("Los 10 numeros siguientes son:\n");

    int i = 1; // Inicializamos i a 1
    while (i <= 10) {
        printf("%d\n", numero + i);
        i++; // Incrementamos i en cada iteraciÃ³n
    }

    return 0;
}
