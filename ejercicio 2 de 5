//programa para obtener los numeros impares de una lista de enteros

#include <stdio.h>
#include <math.h>
int main() 
{
 
int numero;
 
printf("Ingresa un numero: ");
 
scanf("%d", &numero);
 
printf("Numeros impares %d:\n", numero);
 
// Itera desde 1 hasta el número ingresado
 
for (int i = 1; i <= numero; i++) {
 
// Verifica si el número actual (i) es impar
 
if (fmod(i, 2) != 0) //si el i no tiene resuido sera par y no lo mostrara 
 
{
 
printf("%d \n ", i);
 
}
 
}
 
printf("\n");
 
return 0;
