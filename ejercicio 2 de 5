//programa para obtener los numeros impares de una lista de enteros

#include <stdio.h>
#include <math.h>

int main() {
    int numero;

    printf("Ingresa un numero: ");
    scanf("%d", &numero);

    printf("Numeros impares hasta %d:\n", numero);

    int i = 1; // Inicializamos i en 1
    while (i <= numero) {
        printf("%d\n", i);
        i += 2; // Incrementamos i en 2 para obtener nÃºmeros impares
    }

    printf("\n");

    return 0;
}

 
